(define (domain test-domain)
	(:requirements :typing :hierarchy)

	(:types A B C D E)
	
	(:predicates
		(done ?a - A ?b - B ?c - C ?d - D)
		(foo ?e - E)
	)

	(:task task1 :parameters ())

	(:method donothing
		:parameters (?a - A ?b - B ?c - C ?d - D)
		:task (task1)
		:subtasks (and
			(noop ?a ?b ?c ?d)
		)
	)
	
	(:action noop
		:parameters (?a - A ?b - B ?c - C ?d - D)
		:precondition (exists (?e - E) (foo ?e))
		:effect (and
			(done ?a ?b ?c ?d)
		)
	)
)
