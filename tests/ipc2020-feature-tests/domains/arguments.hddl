(define (domain test-domain)
	(:requirements :typing :hierarchy)

	(:types A)
	
	(:predicates
    	(foo ?a - A)
	)

	(:task task1 :parameters ())

	(:method donothing
		:parameters (?a ?b - A)
		:task (task1)
		:subtasks (and
			(noop ?a ?b)
		)
	)
	
	(:action noop
		:parameters (?a ?b - A)
		:precondition (foo ?a ?b)
	)
)
