(define (domain test-domain)
	(:requirements :negative-preconditions :typing :hierarchy :universal-preconditionsss)

	(:types A)
	
	(:predicates
    	(foo ?a - A)
	)

	(:task task1 :parameters ())

	(:method donothing
		:parameters ()
		:task (task1)
		:subtasks (and
			(noop)
		)
	)
	
	(:action noop
		:parameters ()
		:precondition (forall (?a - A) (foo ?a))
	)
)
